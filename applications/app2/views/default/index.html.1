{{extend 'layout.html'}}
<body style="background-color:powderblue;">
<h2>
    previous post
</h2>
<table class = 'table'>
    {{for row in rows:}}
    <tr>
        <td><a href="{{=URL('show',args=row.id)}}">{{=row.title}}</a></td>
        <td>{{=row.body}}</td>
        <td>{{=row.created_on}}</td>
    </tr>
    {{pass}}
</table>
